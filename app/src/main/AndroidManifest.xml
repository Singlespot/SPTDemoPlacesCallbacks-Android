<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.spt.androiddemoplacescallbacks">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.spt.androiddemoplacescallbacks.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.sptproximitykit.ApiKey"
            android:value="ENTER_YOURS_HERE" /> <!-- TODO change with your account's ApiKey-->
        <meta-data
            android:name="com.sptproximitykit.ApiSecret"
            android:value="ENTER_YOURS_HERE" /> <!-- TODO change with your account's ApiSecret-->
    </application>

</manifest>